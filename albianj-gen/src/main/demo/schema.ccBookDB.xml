<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="ccBookDB" class="org.trangram.config.SchemaContext">
        <property name="schema" value="ccBookDB05"/>
        <property name="targetPackage" value="ccBookDB"/>
        <property name="destination" value="ccBookDB"/>
        <property name="tables">
            <list>
                <bean class="org.trangram.config.TableContext" p:tableName="book_novels05"  p:domainObjectName="BookNovel" p:alias="book_novels" p:tasks-ref="groupTask"/>
                <bean class="org.trangram.config.TableContext" p:tableName="book_novels_exp05"  p:domainObjectName="BookNovelExp" p:alias="book_novels_exp" p:tasks-ref="groupTask"/>
            </list>
        </property>
    </bean>

</beans>